{
  "name": "tsmaz",
  "version": "1.0.2",
  "description": "TypeScript port of the smaz string compression library",
  "main": "dist/tsmaz.cjs.js",
  "browser": "dist/tsmaz.umd.js",
  "module": "dist/tsmaz.esm.js",
  "types": "dist/tsmaz.d.ts",
  "files": [
    "dist",
    "tsmaz.ts"
  ],
  "scripts": {
    "clean": "rm -rfv dist build",
    "lint": "tslint -c tslint.json 'tsmaz.ts'",
    "build": "tsc -p .",
    "prebuild": "npm run clean",
    "bundle": "rollup -c rollup.config.js",
    "prebundle": "npm run build",
    "minify": "google-closure-compiler --js=./dist/tsmaz.umd.js --js_output_file=./dist/tsmaz.umd.min.js",
    "preminify": "npm run bundle",
    "prepack": "npm run minify",
    "test": "jest --coverage --no-cache ./tsmaz.test.ts",
    "generate_codebook": "node ./dist/generate_codebook.js",
    "pregenerate_codebook": "npm pack"
  },
  "keywords": [
    "smaz",
    "compression"
  ],
  "author": "RÃ©mi Berson",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "google-closure-compiler": "^20190325.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rollup": "^1.10.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}
